{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "functions": {
      "app.py": {
        "runtime": "python3.9",
        "memory": 1024,
        "maxDuration": 10,
        "handler": "app.app"
      }
    }
  }
  